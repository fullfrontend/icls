{
    "name": "icls",
    "version": "0.0.1",
    "description": "Convert ICLS theme file to prism.css",
    "author": "Cedric Michaux",
    "main": "bin/icls",
    "license": "MIT",
    "bin": {
        "icls": "bin/icls"
    },
    "scripts": {
        "prebuild": "npm run lint",
        "build": "mkdir -p es5/bin && npm run transpile",
        "lint": "eslint src test",
        "lint:fix": "eslint --fix src test",
        "prepublish": "npm run build",
        "test": "mocha",
        "test:coverage": "nyc mocha",
        "test:scrutinizer": "nyc --reporter=clover _mocha ",
        "test:travis": "nyc --reporter=text-lcov _mocha | coveralls",
        "transpile": "babel src --out-dir es5 --source-maps",
        "watch": "babel src --out-dir es5 --source-maps --watch"
    },
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "commander": "^2.11.0",
        "is-iojs": "^1.1.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "chai": "^4.1.2",
        "coveralls": "^3.0.0",
        "eslint": "^4.9.0",
        "mocha": "^4.0.1",
        "nyc": "^11.2.1"
    }
}
