{
    "name": "icls",
    "version": "0.0.1",
    "description": "Convert ICLS theme file to prism.css",
    "author": "Cedric Michaux",
    "main": "index.js",
    "license": "MIT",
    "bin": {
        "icls": "index.js"
    },
    "scripts": {
        "test": "mocha",
        "test:coverage": "istanbul cover _mocha",
        "test:travis": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "test:scrutinizer": "istanbul cover --report clover _mocha ",
        "lint": "eslint src test",
        "lint:fix": "eslint --fix src test"
    },
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "colors": "^1.1.2",
        "commander": "^2.11.0",
        "immutable": "^3.8.2",
        "postcss": "^6.0.13",
        "postcss-conditionals": "^2.1.0",
        "postcss-simple-vars": "^4.1.0",
        "xml-js": "^1.5.1"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "coveralls": "^3.0.0",
        "eslint": "^4.9.0",
        "istanbul": "^0.4.5",
        "mocha": "^4.0.1"
    }
}
